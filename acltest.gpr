with "libadacrypt.gpr";

project AclTest is

   for Source_Dirs use ("test/**");
   for Main use ("test-tests.adb");
   for Object_Dir use "test";
   --for Exec_Dir    use ".";

   package Compiler is
      for Default_Switches ("ada") use ("-O2", "-gnatyM9999", "-gnat12");
   end Compiler;

   package Builder is
      for Default_Switches ("ada") use ("-j4");
   end Builder;

end AclTest;

